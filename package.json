{
  "name": "eatery-bot",
  "version": "1.0.0",
  "description": "The premier discord bot for tracking your favorite lunch establishment.",
  "repository": "git@github.com:NERO2k/eatery-bot.git",
  "license": "AGPL-3.0",
  "author": "NERO2k <nero2k@outlook.com>",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "node dist/main.js",
    "start-dev": "nodemon dist/main.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nodemonConfig": {
    "delay": "200",
    "ignore": [
      "node_modules/*",
      "src/*"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "dependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.2",
    "node-tesseract-ocr": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "tweetnacl": "^1.0.1",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "husky": "^3.0.9",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1",
    "webpack-cli": "^3.3.10"
  }
}
