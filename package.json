{
  "name": "eatery-bot",
  "version": "1.0.0",
  "description": "The premier discord bot for tracking your favorite lunch establishment.",
  "repository": "git@github.com:NERO2k/eatery-bot.git",
  "license": "AGPL-3.0",
  "author": "NERO2k <nero2k@outlook.com>",
  "main": "dist/main.js",
  "scripts": {
    "format": "prettier --write **/*",
    "start": "node --experimental-modules --es-module-specifier-resolution=node src/index.js",
    "start-dev": "nodemon --experimental-modules --es-module-specifier-resolution=node src/index.js"
  },
  "nodemonConfig": {
    "delay": "200",
    "ignore": [
      "node_modules/*",
      "src/*"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.2",
    "node-tesseract-ocr": "^1.0.0",
    "sequelize": "^5.21.2",
    "sqlite3": "^4.1.0",
    "tweetnacl": "^1.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2"
  },
  "type": "module"
}
